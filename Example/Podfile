#use_frameworks!

source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'

target 'YMTBigClassRoom_Example' do
  pod 'YMTBigClassRoom', :path => '../'
  pod 'QMUIKit', '4.0.4'
  pod 'WoodPeckeriOS', :configurations => ['Debug']
  pod 'LookinServer'
  pod 'ReactiveObjC'
  pod 'coobjc'
  pod 'PINCache'

  target 'YMTBigClassRoom_Tests' do
    inherit! :search_paths
  end
end
